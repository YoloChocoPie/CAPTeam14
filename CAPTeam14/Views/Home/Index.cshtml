<div class="content-wrapper">
    <!-- Main content -->
    <section class="content">
        <div class="bgtitle">Thời khóa biểu</div>

        <div width="100%" class="form-inline col-md-12 magin-bottom-5px" style="margin-bottom:10px">
            <div class="col-md-4 form-group">
                <label>Giảng viên:</label>
                <select class="form-control width-70" id="chongv">
                    <option> Tất cả</option>
                    @foreach (var itemgv in db.nguoiDungs.Where(x => x.role == 4))
                    {
                        <option value="@itemgv.ID">@itemgv.tenGV</option>
                    }
                </select>
            </div>

            <div class="col-md-4 form-group">
                <label>Học kỳ:</label>
                <select class="form-control width-70" id="chonhk">
                    <option value="koALL" selected>-- Chọn Học Kỳ ---</option>
                    @foreach (var item in db.hocKies)
                    {
                        <option value="@item.ID">@item.tenHK</option>
                    }
                </select>
            </div>

            <div class="col-md-4 form-group" id="cbTweek">
                <label>Tuần:</label>
                <select class="form-control width-70" id="Week" name="Week" @*onchange="TabTranferTabPerior(0)"*@>
                    @foreach (var tuan in db.tuanHocs.Select(x => x.tuanHoc1).Distinct())
                    {
                        string cc = tuan;
                        string[] cl = cc.Split(',', ';', ' ');

                        foreach (var clm in cl)
                        {
                            <option value="@clm">@clm</option>
                        }

                    }
                </select>
            </div>
            <div class="col-md-4" id="btnPrint" style="display: none">
                <a href="javascript:void(0)" class="btn btn-link" onclick="PrintTKB()">[In TKB]</a>
            </div>
        </div>

        <div width="100%" class="form-inline col-md-12 magin-bottom-5px">
            <div class="col-md-4 form-group">
                <label>Trạng Thái:</label>
                <select class="form-control width-70" onchange="FilterStatus(this)">
                    <option value="KoPhancong">Chưa phân công</option>
                    <option value="DaPhancong">Đã Phân công</option>
                </select>
            </div>

            <div class="col-md-4 form-group">
                <label>Loại HP:</label>
                <select class="form-control width-70" onchange="FilterLoaiHP(this)">
                    <option value="tatca">Tất cả</option>
                    <option value="LyThuyet">Lý Thuyết</option>
                    <option value="ThucHanh">Thực Hành</option>
                </select>
            </div>

            <div class="col-md-4 form-group" id="cbTweek">
                <label>Môn học:</label>
                <select class="form-control width-70" id="chonmonhox">
                    <option value="All" selected="selected">Tất cả</option>
                    @foreach (var mhx in db.monHocs.Select(x => x.tenMon).OrderBy(x => x))
