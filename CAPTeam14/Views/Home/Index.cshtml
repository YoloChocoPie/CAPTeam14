<div class="content-wrapper">
    <!-- Main content -->
    <section class="content">
        <div class="bgtitle">Thời khóa biểu</div>

        <div width="100%" class="form-inline col-md-12 magin-bottom-5px" style="margin-bottom:10px">
            <div class="col-md-4 form-group">
                <label>Năm học:</label>
                <select class="form-control width-70" id="YearStudy" name="YearStudy" onchange="LoadWeek()">
                    <option value="2023-2024">2023-2024</option>
                    <option value="2022-2023">2022-2023</option>
                    <option selected="selected" value="2021-2022">
                        2021-2022
                    </option>
                    <option value="2020-2021">2020-2021</option>
                </select>
            </div>

            <div class="col-md-4 form-group">
                <label>Học kỳ:</label>
                <select class="form-control width-70" id="TermID" onchange="LoadWeek()">
                    <option value="HK01">Học kỳ 1</option>
                    <option selected="selected" value="HK02">
                        Học kỳ 2
                    </option>
                    <option value="HK03">Học kỳ 3</option>
                </select>
            </div>

            <div class="col-md-4 form-group" id="cbTweek">
                <label>Tuần:</label>
                <select class="form-control width-70" id="Week" name="Week" onchange="TabTranferTabPerior(0)">

                    @foreach (var tuan in db.tuanHocs.Select(x => x.tuanHoc1).Distinct())
                    {
                        string cc = tuan;
                        string[] cl = cc.Split(',', ';', ' ');

                        foreach (var clm in cl)
                        {
                            <option value="@clm">@clm</option>
                        }

                    }
                </select>
            </div>
            <div class="col-md-4" id="btnPrint" style="display: none">
                <a href="javascript:void(0)" class="btn btn-link" onclick="PrintTKB()">[In TKB]</a>
            </div>
        </div>

        <div width="100%" class="form-inline col-md-12 magin-bottom-5px">
            <div class="col-md-4 form-group">
                <label>Trạng Thái:</label>
                <select class="form-control width-70" id="YearStudy" name="YearStudy" onchange="LoadWeek()">
                    <option value="">Chưa phân công</option>
                    <option value="">Đã Phân công</option>
                </select>
            </div>

            <div class="col-md-4 form-group">
                <label>Loại HP:</label>
                <select class="form-control width-70" onchange="FilterLoaiHP(this)">
                    <option value="tatca">Tất cả</option>
                    <option value="LyThuyet">Lý Thuyết</option>
                    <option value="ThucHanh">Thực Hành</option>
                </select>
            </div>

            <div class="col-md-4 form-group" id="cbTweek">
                <label>Môn học:</label>
                <select class="form-control width-70" id="Week" name="Week" onchange="Filtermonhox()">
                    <option value="" selected="selected">Tất cả</option>
                    @foreach (var mhx in db.monHocs.Select(x => x.tenMon).OrderBy(x => x))
                    {
                        //string mh0x = mhx.tenMon;
                        //string[] dmdoi = mh0x.ToArray();
                        //Array.Sort(dmdoi);
                        <option value="@mhx">@mhx</option>
                    }
                </select>
            </div>

            <div class="col-md-4" id="btnPrint" style="display: none">
                <a href="javascript:void(0)" class="btn btn-link" onclick="PrintTKB()">[In TKB]</a>
            </div>
        </div>

