                        //string mh0x = mhx.tenMon;
                        //string[] dmdoi = mh0x.ToArray();
                        //Array.Sort(dmdoi);
                        <option value="@mhx">@mhx</option>
                    }
                </select>
            </div>

            <div class="col-md-4" id="btnPrint" style="display: none">
                <a href="javascript:void(0)" class="btn btn-link" onclick="PrintTKB()">[In TKB]</a>
            </div>
        </div>

