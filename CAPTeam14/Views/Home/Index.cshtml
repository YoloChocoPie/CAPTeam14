        // Lọc loại HP
        function FilterLoaiHP(select) {
            //var x = document.getElementById("ChonLHP").value;
            if (select.value == "tatca") {
                targList = document.getElementsByClassName("formTT");
                tarzan = document.getElementsByClassName("formLT");
                if (targList) {
                    for (var x = 0; x < targList.length; x++) {
                        for (var y = 0; y < tarzan.length; y++) {

                            tarzan[y].style.display = ""
                        }
                        targList[x].style.display = "";
                    }
                }

            } else if (select.value == "LyThuyet") {
                targList = document.getElementsByClassName("formTT");
                tarzan = document.getElementsByClassName("formLT");
                if (targList) {
                    for (var x = 0; x < targList.length; x++) {
                        for (var y = 0; y < tarzan.length; y++) {

                            //tarzan[y].style.visibility = "visible"
                            tarzan[y].style.display = ""
                        }
                        targList[x].style.display = "none";
                    }
                }

                //document.getElementsByClassName("formLT").style.visibility = "visible";
                //document.getElementsByClassName("formTT").style.visibility = "hidden";
            } else if (select.value == "ThucHanh") {
                targList = document.getElementsByClassName("formLT");
                tarzan = document.getElementsByClassName("formTT");
                if (targList) {
                    for (var x = 0; x < targList.length; x++) {
                        for (var y = 0; y < tarzan.length; y++) {

                            //tarzan[y].style.visibility = "visible"
                            tarzan[y].style.display = ""
                        }
                        targList[x].style.display = "none";
                    }
                }

                //document.getElementsByClassName("formLT").style.visibility = "hidden";
                //document.getElementsByClassName("formTT").style.visibility = "visible";
            }
        }

        // Lọc Môn học
        $(document).ready(function () {
            $('#chonmonhox').on('change', function () {
                var value = $(this).val();
                $('tr').each(function () {
                    var id = $(this).attr('name');
                    /*alert(id); // check xem đã lấy dc giá trị của dropdownlist hay chưa*/
                    if (value == id) {
                        //$('.test').css("display", "none");
                        //$(this).css("display", "");
                        $('.test').hide();
                        $(this).show();
                    }
                });
                if (value == "All") {
                    //$('.test').css("display", "");
                    $('.test').show();
                }
            });
        });

        // PHAN CONG _ AJAX hoạt động được
        $('form').submit(function (e) {
            e.preventDefault();
            $.ajax({
                method: $(this).attr('method'),
                url: $(this).attr('action'),
                data: $(this).serialize(),
                success: function (res) {
                    if (res.result == true) {
                        Swal.fire({
                            title: 'Thông báo',
                            text: 'Bạn đã phân công thành công !!!',
                            icon: 'success',
                            showConfirmButton: true,
                        });
                    } else {
                        Swal.fire({
                            title: 'Thông báo',
                            text: 'Giảng viên này đã được phân công quá nhiều !!!',
                            icon: 'error',
                            showConfirmButton: true,
                        });
                    }
                }
            });
        });


