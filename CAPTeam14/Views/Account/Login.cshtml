@using CAPTeam14.Models
@model LoginViewModel
@{
    Layout = null;

}

<!DOCTYPE html>

<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">

    <!-- Mobile Metas -->
